@ARTICLE{dropseq,
  title    = "Highly Parallel Genome-wide Expression Profiling of Individual
              Cells Using Nanoliter Droplets",
  author   = "Macosko, Evan Z and Basu, Anindita and Satija, Rahul and Nemesh,
