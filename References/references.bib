@ARTICLE{atoi,
  title    = "Adenosine-to-inosine {RNA} editing",
  author   = "Zinshteyn, Boris and Nishikura, Kazuko",
  abstract = "Ribonucleic acid (RNA) editing is a mechanism that generates RNA
              and protein diversity, which is not directly encoded in the
              genome. The most common type of RNA editing in vertebrates is the
